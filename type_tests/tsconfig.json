/* Config used for dtslint unit tests. */
{
    "compilerOptions": {
        /* Basic Options */
        "target": "es5",
        "module": "commonjs",

        "lib": ["es6", "dom"],

        "downlevelIteration": true,

        /* Strict Type-Checking Options */
        "strict": true,
        "alwaysStrict": true,

        /* Module Resolution Options */
        "moduleResolution": "node",
        "baseUrl": "./",
        "types": []
    }
}
